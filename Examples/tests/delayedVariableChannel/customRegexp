val1In {
       theTitle "Values";
       titles ("val1 on inlet");
       expr "Expression var1Val on inlet:  min=.+ max=.+ average=(%f%)";
}
val2In {
       type slave;
       master val1In;
       titles ("val2 on inlet");
       expr "Expression var2Val on inlet:  min=.+ max=.+ average=(%f%)";
}